{
  "name": "threed-tictactoe",
  "version": "0.1.0",
  "description": "A 3D Tic-Tac-Toe multiplayer game built with React, TypeScript, and Three.js",
  "keywords": [
    "Tic-Tac-Toe",
    "3D game",
    "multiplayer",
    "React",
    "Three.js",
    "WebSockets"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "three": "^0.176.0",
    "@react-three/fiber": "^9.1.2",
    "@react-three/drei": "^10.0.7",
    "framer-motion": "^12.9.7",
    "tailwindcss": "^4.1.5"
  },
  "devDependencies": {
    "vite": "^6.3.5",
    "@vitejs/plugin-react": "^4.4.1",
    "typescript": "^5.8.3",
    "prettier": "^3.5.3",
    "eslint-config-prettier": "^10.1.2",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "eslint-plugin-react": "^7.37.5",
    "@testing-library/react": "^16.3.0",
    "ts-jest": "^29.3.2",
    "jsdom": "^26.1.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}"
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}